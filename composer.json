{
    "name":"toflar/contao-css-class-replacer",
    "description":"Replace any CSS class of your page elements by using any CSS selector",
    "keywords":["contao", "css", "front end", "replace"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"Yanick Witschi",
            "email":"yanick.witschi@terminal42.ch",
            "homepage":"http://terminal42.ch"
        }
    ],
    "require":{
        "php":">=5.3.2",
        "contao/core":">=3.2.13,<3.5",
        "contao-community-alliance/composer-plugin":"2.*",
        "symfony/css-selector":"2.5.*",
        "symfony/stopwatch":"2.5.*"
    },
    "autoload": {
        "psr-4": {
            "Toflar\\Contao\\CssClassReplacer\\": "src"
        }
    },
    "extra":{
        "contao": {
            "sources":{
                "contao-module": "system/modules/css-class-replacer"
            }
        }
    }
}
